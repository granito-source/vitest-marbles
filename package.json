{
  "name": "@granito/vitest-marbles",
  "version": "1.0.0-dev.2",
  "description": "Marble testing helpers library for RxJs and Vitest",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/granito-source/vitest-marbles.git"
  },
  "license": "MIT",
  "keywords": [
    "vitest",
    "marbles",
    "marble",
    "testing",
    "test",
    "rxjs",
    "observable"
  ],
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest --no-watch"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "devDependencies": {
    "jsdom": "^27.2.0",
    "rxjs": "^7.8.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "peerDependencies": {
    "rxjs": "^7.0.0",
    "vitest": "^4.0.0"
  }
}
