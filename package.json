{
  "name": "@granito/vitest-marbles",
  "version": "1.0.0-dev.0",
  "description": "Marble testing helpers library for RxJs and Vitest",
  "main": "dist/umd/jest-marbles.js",
  "module": "dist/lib/index.js",
  "es2015": "dist/lib-esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/granito-source/vitest-marbles"
  },
  "author": "Evgeny Barabanov",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "vitest",
    "marbles",
    "marble",
    "testing",
    "test",
    "rxjs",
    "observable"
  ],
  "scripts": {
    "e2e": "jest --config=e2e/jest-browser.config.js && jest --config=e2e/jest-node.config.js",
    "build": "tsc && tsc --target es2017 --outDir dist/lib-esm && webpack && webpack --env prod",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": "/spec/.*\\.(ts|tsx|js)$"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rxjs": "^7.8.2",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.3.1",
    "typescript": "^4.9.5",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-config-utils": "^2.3.1"
  },
  "peerDependencies": {
    "rxjs": "^7.0.0"
  }
}
